          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="avg_sale">
          <DataField>avg_sale</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="max_sale_per_day">
          <DataField>max_sale_per_day</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
