  <DataSets>
    <DataSet Name="daily_room_stat">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@room_number">
            <Value>=Parameters!room_number.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@start_date">
            <Value>=Parameters!start_date.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@end_date">
            <Value>=Parameters!end_date.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT  r.room_number, s.status_name, f.date_id,s.status_id
FROM     hotel.fact_daily_hotel  f JOIN
                  hotel.dim_room r ON f.room_key=r.room_key
                   JOIN
                  hotel.dim_room_status s on s.status_id=r.status_id
WHERE  (r.room_number = @room_number)
and date_id&gt;=ISNULL(@start_date,'1900-01-01')
and date_id&lt;=ISNULL(@end_date,'2200-01-01')</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="room_number">
          <DataField>room_number</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="status_name">
          <DataField>status_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="date_id">
          <DataField>date_id</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="status_id">
          <DataField>status_id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="param__room_number">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select distinct(room_number) from hotel.dim_room</CommandText>
      </Query>
      <Fields>
        <Field Name="room_number">
          <DataField>room_number</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
